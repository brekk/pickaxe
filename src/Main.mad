import Dir from "Directory"
import IO from "IO"
import Process from "Process"
import Wish from "Wish"

import Folder from "@/Folder"



main = () => {
  pipe(
    mappend($, "/madlib.json"),
    IO.pTrace("directory?"),
    Folder.isDirectory,
    Wish.fulfill((e) => { IO.trace("bad", e) }, (x) => { IO.trace("good", x) }),
  )(Process.getCurrentWorkingDirectory())
}
